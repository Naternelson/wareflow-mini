		"@types/jest": "^27.5.2",
		"@types/node": "^16.18.53",
		"@types/react": "^18.2.22",
		"@types/react-dom": "^18.2.7",
		"bcrypt": "^5.1.1",
		"dotenv": "^16.3.1",
		"electron-devtools-installer": "^3.2.0",
		"electron-is-dev": "^2.0.0",
		"jsonwebtoken": "^9.0.2",
		"pdf-to-printer": "^5.6.0",
		"pdf2pic": "^3.0.3",
		"pluralize": "^8.0.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.47.0",
		"react-redux": "^8.1.2",
		"react-router-dom": "^6.16.0",
		"react-scripts": "5.0.1",
		"redux-devtools-extension": "^2.13.9",
		"reselect": "^4.1.8",
		"sequelize": "^6.33.0",
		"sqlite3": "^5.1.6",
		"typeface-inter": "^3.18.1",
		"typescript": "^4.9.5",
		"uuid": "^9.0.1",
		"web-vitals": "^2.1.4"
	},
	"scripts": {
		"start-electron": "electron -r ts-node/register ./electron.ts",
		"start-react": "cross-env BROWSER=none react-scripts start",
		"start": "concurrently \"npm run start-electron\" \"npm run start-react\"",
		"build": "react-scripts build",
		"eject": "react-scripts eject",
		"test": "jest",
		"electron-pack": "electron-builder build --win --mac --linux"
	},
	"build": {
		"appId": "com.example.mynewapp",
		"files": [
			"build/**/*",
			"electron.ts"
		],
		"directories": {
			"buildResources": "assets"
		}
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@babel/plugin-proposal-private-property-in-object": "^7.21.11",
		"@babel/plugin-transform-class-properties": "^7.22.5",
		"@babel/plugin-transform-private-methods": "^7.22.5",
		"@babel/preset-typescript": "^7.23.2",
		"@faker-js/faker": "^8.2.0",
		"@types/bcrypt": "^5.0.0",
		"@types/electron": "^1.6.10",
		"@types/jsonwebtoken": "^9.0.3",
		"@types/pluralize": "^0.0.31",
		"@types/react-router-dom": "^5.3.3",
		"@types/sequelize": "^4.28.16",
		"@types/sqlite3": "^3.1.9",
		"@types/uuid": "^9.0.4",
		"concurrently": "^8.2.1",
		"cross-env": "^7.0.3",
		"electron": "^26.2.2",
		"electron-builder": "^24.6.4",
		"electron-rebuild": "^3.2.9",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.1",
		"wait-on": "^7.0.1"
	},
	"include": [
		"src/**/*",
		"types/**/*"
	]
}
